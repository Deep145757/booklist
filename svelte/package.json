{
  "name": "booklist",
  "main": "app.js",
  "description": "A book management web app for exploring modern web technologies.  Think ToDo, but more realistic",
  "version": "0.1.0",
  "scripts": {
    "start": "webpack -w",
    "webpack": "webpack",
    "tsc": "svelte-check --threshold error && tsc --noEmit",
    "svelte-check": "svelte-check --watch --threshold error",
    "tscw": "tsc -w --noEmit",
    "deploy": "cd .. && npm run deploy",
    "rollup-sw": "cd ./service-worker && rollup -c -w",
    "persistgraphql": "persistgraphql graphQL",
    "refresh": "npm run persistgraphql && npm start"
  },
  "dependencies": {
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-typescript": "^7.3.3",
    "babel-loader": "^8.0.5",
    "classnames": "2.2.5",
    "css-loader": "^2.1.1",
    "d3-array": "1.2.0",
    "d3-axis": "1.0.8",
    "d3-scale": "1.0.6",
    "d3-selection": "1.1.0",
    "d3-transition": "1.1.0",
    "dotenv-webpack": "^1.7.0",
    "file-loader": "^3.0.1",
    "history": "^5.0.0",
    "html-webpack-plugin": "^4.3.0",
    "lodash.debounce": "4.0.8",
    "micro-graphql-svelte": "^0.2.0-beta1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.14.1",
    "persistgraphql": "^0.3.11",
    "purify-css": "1.2.2",
    "query-string": "4.3.2",
    "rollup": "^0.67.3",
    "rollup-plugin-babel-minify": "^6.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.0",
    "sass-loader": "^7.1.0",
    "shallow-equal": "^1.0.0",
    "svelte": "^3.34.0",
    "svelte-check": "^1.1.18",
    "svelte-file-dropzone": "0.0.15",
    "svelte-helpers": "^0.2.0-beta23",
    "svelte-loadable": "^1.5.1",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^4.6.1",
    "terser-webpack-plugin": "^1.2.3",
    "toastify-js": "^1.3.1",
    "typescript": "3.8.3",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "workbox-webpack-plugin": "5.1.3"
  },
  "devDependencies": {
    "prettier-plugin-svelte": "^1.4.1"
  }
}
