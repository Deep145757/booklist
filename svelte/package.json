{
  "name": "booklist",
  "main": "app.js",
  "description": "A book management web app for exploring modern web technologies.  Think ToDo, but more realistic",
  "version": "0.1.0",
  "scripts": {
    "start": "webpack -w",
    "webpack": "webpack",
    "tsc": "svelte-check --threshold error && tsc --noEmit",
    "svelte-check": "svelte-check --watch --threshold error",
    "tscw": "tsc -w --noEmit",
    "deploy": "cd .. && npm run deploy",
    "rollup-sw": "cd ./service-worker && rollup -c -w",
    "persistgraphql": "persistgraphql graphQL",
    "refresh": "npm run persistgraphql && npm start"
  },
  "dependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@types/node": "^16.11.10",
    "classnames": "2.2.5",
    "d3-array": "1.2.0",
    "d3-axis": "1.0.8",
    "d3-scale": "1.0.6",
    "d3-selection": "1.1.0",
    "d3-transition": "1.1.0",
    "history": "^5.0.0",
    "lodash.debounce": "4.0.8",
    "micro-graphql-svelte": "^0.2.2",
    "node-sass": "^4.14.1",
    "persistgraphql": "^0.3.11",
    "purify-css": "1.2.2",
    "query-string": "4.3.2",
    "shallow-equal": "^1.0.0",
    "svelte": "^3.42.5",
    "svelte-check": "^1.1.18",
    "svelte-file-dropzone": "0.0.15",
    "svelte-helpers": "^0.2.1",
    "svelte-loadable": "^1.5.1",
    "svelte-preprocess": "^4.6.1",
    "toastify-js": "^1.11.1",
    "typescript": "3.8.3",
    "vite": "^2.6.14",
    "vite-plugin-pwa": "^0.11.7"
  },
  "devDependencies": {
    "prettier-plugin-svelte": "^1.4.1"
  }
}
