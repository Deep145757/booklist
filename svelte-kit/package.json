    "typescript": "^4.7.4",
    "vite": "^3.1.0"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/kit": "^1.0.0-next.571",
    "classnames": "^2.3.2",
    "d3-array": "1.2.0",
    "d3-axis": "1.0.8",
    "d3-scale": "1.0.6",
    "d3-selection": "1.1.0",
