{
  "name": "booklist",
  "main": "app.js",
  "description": "A book management web app for exploring modern web technologies.  Think ToDo, but more realistic",
  "version": "0.1.0",
  "scripts": {
    "start": "vite",
    "dev-all": "concurrently npm:dev npm:dev-sw --prefixColors blue,green",
    "dev-sw": "vite build --watch --config vite-service-worker.config.ts",
    "build": "vite build",
    "build-sw": "vite build --config vite-service-worker.config.ts",
    "preview": "vite preview",
    "tscw": "tsc -w --noEmit",
    "deploy": "cd .. && npm run deploy",
    "persistgraphql": "persistgraphql graphQL",
    "check-circular": "madge --circular reactStartup.ts"
  },
  "dependencies": {
    "@reach/dialog": "^0.8.2",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@vitejs/plugin-react": "^1.0.9",
    "classnames": "2.2.5",
    "concurrently": "^6.4.0",
    "d3-array": "1.2.0",
    "d3-axis": "1.0.8",
    "d3-scale": "1.0.6",
    "d3-selection": "1.1.0",
    "d3-transition": "1.1.0",
    "dotenv": "^10.0.0",
    "history": "4.6.1",
    "lodash.debounce": "4.0.8",
    "madge": "^5.0.1",
    "micro-graphql-react": "^0.4.0-rc.9",
    "persistgraphql": "^0.3.11",
    "purify-css": "1.2.2",
    "query-string": "4.3.2",
    "react": "18.0.0-alpha-3c4c1c470-20211021",
    "react-autosuggest": "9.3.2",
    "react-dom": "18.0.0-alpha-3c4c1c470-20211021",
    "react-dropzone": "4.2.5",
    "react-measure": "^2.5.2",
    "react-spring": "^9.0.0-rc.3",
    "sass": "^1.43.4",
    "shallow-equal": "^1.0.0",
    "toastify-js": "^1.11.1",
    "typescript": "3.8.3",
    "vite": "^2.6.14",
    "vite-plugin-pwa": "^0.11.5"
  }
}
